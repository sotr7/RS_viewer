				})
			})
		},
		parseNewsCats: function (a, b, c, d) {
			var e,
			f = 0,
			g = [],
			h = !0,
			i = a.categoryId,
			j = RS3.newsCategories();
			switch (archiveOrNewsPage) {
			case "archive":
				e = "jptg=ia&jptv=news_archive";
				break;
			case "news":
				e = "jptg=ia&jptv=news_list";
				break;
			case "home":
				e = "jptg=ia&jptv=community_news"
			}
			null !== a && a.newsItems.length && ($.each(a.newsItems, function (a, c) {
					var d = 0 === i ? c.categoryId : i,
					h = j[d];
					return RS3.global.categoryDropDown.fallbackImages(d),
					"video" === b && 3 != c.largeMediaType || !(f > 7) && (f += 1, g.push("<article " + (3 == c.largeMediaType ? 'class="video"' : "") + ">"), g.push('<figure><a href="' + c.link + "?" + e + '">'), g.push('<img src="' + ("" != c.summaryImageLink ? c.summaryImageLink : fallbackImage) + '" alt="' + c.title + 'Teaser Image" title="' + c.title + 'Teaser Image" />'), g.push("</a></figure>"), g.push('<div class="copy"><header>'), g.push('<h4><a href="' + c.link + (3 == c.largeMediaType ? "?play=true" : "") + "&" + e + '">' + c.title + "</a></h4>"), g.push('<h5><a href="' + ajaxNewsUrl + "/list?cat=" + d + '&page=1">'), g.push(h), g.push("</a></h5>"), g.push("<time>" + c.formattedDate + "</time></header>"), "home" != archiveOrNewsPage && g.push("<p>" + c.summary + ' <a class="readMore" href="' + c.link + "?" + e + '">Read More</a></p>'), g.push("</div>"), void g.push("</article>"))
				}), 0 != f && (h = !1)),
			h && g.push('<p class="error">Sorry, there are no articles for this category. Please try another category.</p>'),
			c.html(g.join("")),
			c.fadeIn(500)
		}
	},
