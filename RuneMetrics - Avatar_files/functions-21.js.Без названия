			"use strict";
			var c = a || PAGEGLOBALS.FB.queryString;
			"undefined" == typeof FB || "function" != typeof FB.getAuthResponse ? alert("Couldn't contact Facebook. Please try again later.") : null == FB.getAuthResponse() ? FB.login(function (a) {
