		$("#gameLaunchNotLoad").on("click", function () {
			RS3.gamePage.failure()
		}),
		RS3.gamePageBrowser.checkJava();
		var a = 1e3;
		JXGLOBAL.user.browser().isIE && (a = 5e3);
