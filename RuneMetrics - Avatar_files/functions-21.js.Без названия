	addCToLink: function (a) {
		var b = function (a) {
			var b,
			c,
			d = a.split("/");
			for (var e in d)
				c = d[e], c.split("c=").length > 1 && (b = c);
			return b
		}
		(pageLocation);
		return a = function (a) {
			var b,
			c = a.split("/"),
			d = [];
