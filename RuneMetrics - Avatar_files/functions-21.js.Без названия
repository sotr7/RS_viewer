			associationForm: $("#associationForm"),
			promptMessage: $("#associationForm #associatePrompt"),
			usernameField: $("#associationForm #username"),
			passwordField: $("#associationForm #password"),
			assocSubmitTrigger: $("#associationForm #existingplayer")
		};
		RS3.assoc.dom = a,
		RS3.assoc.dom.showAssocTrigger.on("click", function (a) {
			a.preventDefault(),
			RS3.assoc.showAssoc()
		}),
		RS3.assoc.dom.assocSubmitTrigger.on("click", function (a) {
			a.preventDefault(),
			RS3.assoc.checkForm()
		}),
		assocVar.errorCode > 0 && (RS3.assoc.showAssoc(), RS3.assoc.showErrors())
	}
