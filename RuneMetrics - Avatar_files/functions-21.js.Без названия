	playerCount: function () {
		var a = $("#playerCount");
		if (a.length > 0) {
			var b = parseInt(a.html().replace(",", ""));
			(b < 1 || isNaN(b)) && RS3.global.pollPlayerCount(a)
		}
	},
	pollPlayerCount: function (a) {
		$.ajax({
			url: baseURL + "/player_count.js?varname=iPlayerCount",
			dataType: "jsonp",
			success: function (b) {
				b > 0 && a.html(RS3.global.playerCountCommas(b))
			}
		})
	},
	playerCountCommas: function (a) {
		a += "",
		x = a.split("."),
		x1 = x[0],
		x2 = x.length > 1 ? "." + x[1] : "";
		for (var b = /(\d+)(\d{3})/; b.test(x1); )
			x1 = x1.replace(b, "$1,$2");
		return x1 + x2
	},
